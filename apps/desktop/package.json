    "@follow/utils": "workspace:*",
    "@pengx17/electron-forge-maker-appimage": "1.2.1",
    "@types/html-minifier-terser": "7.0.2",
    "@types/js-yaml": "4.0.9",
    "@vitejs/plugin-react": "5.1.4",
    "async-es": "3.2.6",
