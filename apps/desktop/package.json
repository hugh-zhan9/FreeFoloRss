    "vite-bundle-analyzer": "1.3.6",
    "vite-plugin-mkcert": "1.17.9",
    "vite-plugin-pwa": "1.2.0",
    "vite-plugin-route-builder": "0.4.1",
    "vite-tsconfig-paths": "6.1.1"
