{
  "name": "FreeFolo",
  "type": "module",
  "version": "1.3.1",
  "private": true,
  "description": "FreeFolo desktop RSS reader",
  "author": "FreeFolo Team",
  "homepage": "https://github.com/RSSNext",
  "repository": {
    "url": "https://github.com/RSSNext/Folo",
    "type": "git"
  },
  "main": "./dist/main/index.js",
  "scripts": {
    "analyze:web": "cross-env analyzer=1 WEB_BUILD=1 vite build",
    "build:electron": "electron-vite build && electron-forge make",
    "build:electron:unsigned": "FOLO_NO_SIGN=1 electron-vite build && FOLO_NO_SIGN=1 electron-forge make",
    "build:electron-forge": "electron-forge make",
    "build:electron-forge:macos": "electron-forge make --arch=x64 --platform=darwin && electron-forge make --arch=arm64 --platform=darwin && tsx scripts/merge-yml.ts",
    "build:electron-forge:mas": "electron-forge make --arch=universal --platform=mas",
    "build:electron-forge:ms": "tsx scripts/generate-appx-manifest.ts && electron-forge make --platform=win32 --ms=true",
    "build:electron-vite": "electron-vite build",
    "build:render": "vite build -c vite.config.electron-render.ts",
    "build:web": "rm -rf out/web && cross-env WEB_BUILD=1 vite build",
    "bump": "vv --minor",
    "dedupe:locales": "eslint --fix locales/**/*.json",
    "depcheck": "npx depcheck --quiet",
    "dev": "turbo run @follow/web#dev",
    "dev:debug": "export DEBUG=true && vite --debug",
    "dev:electron": "electron-vite dev",
    "dev:web": "cross-env WEB_BUILD=1 vite",
    "hotfix": "vv -c bump.hotfix.config.js --patch",
    "publish": "electron-vite build && electron-forge publish",
    "start": "electron-vite preview",
    "update:main-hash": "tsx plugins/vite/generate-main-hash.ts"
  },
  "dependencies": {
    "@protobufjs/aspromise": "1.1.2",
    "@protobufjs/eventemitter": "1.1.0",
    "@protobufjs/fetch": "1.1.0",
    "@protobufjs/float": "1.0.2",
    "@protobufjs/inquire": "1.1.0",
    "@protobufjs/path": "1.1.2",
    "@protobufjs/pool": "1.1.0",
    "@protobufjs/utf8": "1.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.11.1",
    "@electron-forge/maker-appx": "7.11.1",
    "@electron-forge/maker-dmg": "7.11.1",
    "@electron-forge/maker-pkg": "7.11.1",
    "@electron-forge/maker-squirrel": "7.11.1",
    "@electron-forge/maker-zip": "7.11.1",
    "@electron-forge/plugin-fuses": "7.11.1",
    "@electron-forge/publisher-github": "7.11.1",
    "@electron-toolkit/tsconfig": "2.0.0",
    "@follow/components": "workspace:*",
